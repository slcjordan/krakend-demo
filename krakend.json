{
  "version": 2,
  "extra_config": {},
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "name": "myservice",
  "port": 8080,
  "disable_rest": false,
  "endpoints": [
    {
      "endpoint": "/",
      "method": "GET",
      "output_encoding": "json",
      "extra_config": {},
      "backend": [
        {
          "url_pattern": "/response.json",
          "encoding": "json",
          "method": "GET",
          "extra_config": {},
          "host": [
            "http://mynginx"
          ],
          "disable_host_sanitize": true
        },
        {
          "url_pattern": "/whatever.json",
          "encoding": "json",
          "method": "GET",
          "extra_config": {},
          "host": [
            "http://mynginx"
          ],
          "disable_host_sanitize": true
        }
      ]
    }
  ]
}
